{
  "compilerOptions": {
    "target": "es5",
    // μ„Έν… 1
    "baseUrl": "./src",  
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ],
  //μ„Έν…2
  "paths": {
    "pages/*": [
      "pages/*"
    ],
    "components/*": [
      "components/*"
    ],
    "firebaseApp/*": [
      "firebaseApp/*"
    ],
    "context/*": [
      "context/*"
    ]
  } 
}
